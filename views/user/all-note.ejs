<body>
	
<div id="slider">
	<div id="tooplate_wrapper">
        <div id="tooplate_sidebar">
		
            <div id="header">
              <h1><a href="#"><img src="../../images/ribbon.gif" style="height: 118px; width: 210px; margin-top: -30px; border-radius: 60px;" title = "Notebook Template - tooplate.com" alt="Notebook free html template" /></a></h1>
            </div>    
			
            <div id="menu">
                <ul class="navigation">
                  <li><a href="/home" class="selected menu_01">Home</a></li>
                  <li><a href="/write-note" class="menu_02">Take Note</a></li>
                  <li><a href="/mynotes" class="menu_03">Archive</a></li>
                  <li><a href="/update-details" class="menu_04">Update Profile</a></li>
                  <li><a href="/logout" class="menu_05">Logout</a></li>
                </ul>
            </div>
		</div> <!-- end of sidebar -->  

    <div id="content">
      <div class="scroll">
        <div class="scrollContainer">    
<div class="notes content">
  <h2>My Notes</h2>

<% if(notes.length > 0) { %>
  <% notes.forEach(note => { %>
<a class="single" href="/note/<%= note._id %>" >
  <h3 class="title"><%= note.title.substring(0, 40)  %><% if(note.title.length  >= 40){ %>...<%}else { %><% } %></h3>
  <p class="body">
    <%= note.body.substring(0, 40)%><% if(note.body.length  >= 40){ %>...<%}else { %><% } %>
  </p>
  <p><%= note.timeAdded %></p>
  
</a>
  <% }) %>
<% } else{ %>
  <p>There are no blogs to display.....</p>
<% } %>



</div><!-- end of scroll -->
</div>
</div> <!-- end of content -->
</div>



<div id="footer">
        
