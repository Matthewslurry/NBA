<style>
  .error_msg{
      color: #a94442;
      background-color: #f2dede;
      border-color: #ebccd1;
      text-align: center;
      
  }

  .success_msg{
      color: #3c763d;
      background-color: #ddf0d8;
      border-color: #d6e9c6;
      text-align: center;
  }

  .notes a {
    display: block;
    margin: 40px 0;
    padding-left: 30px;
    border-left: 6px solid crimson;
    
  }
  .notes a:hover h3 {
    color: crimson;
  }

  </style>

